	<subDataset name="dataset" uuid="371698e7-4549-42ca-ace0-5c6bbc551995">
		<parameter name="dId" class="java.lang.Long"/>
		<parameter name="sDate" class="java.lang.String"/>
		<parameter name="eDate" class="java.lang.String"/>
		<parameter name="tlist" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
  COUNT(o.total_fee) AS c,
  SUM(o.total_fee)   AS fee,
  t.`name`           AS name,
  d.`name`           AS dName
FROM (tenant AS t
  LEFT JOIN pay_order AS o ON (t.id = o.tenant_id)
                              AND o.status = 'SUCCESS'
                              AND DATEDIFF(DATE_FORMAT(o.time_end, '%Y-%m-%d'), $P{sDate}) >= 0
                              AND DATEDIFF(DATE_FORMAT(o.time_end, '%Y-%m-%d'), $P{eDate}) <= 0
  ), department AS d
WHERE d.id = $P{dId}
      AND t.department_id = d.id
      AND t.id IN ($P!{tlist})
GROUP BY t.id
ORDER BY t.code]]>
		</queryString>
		<field name="c" class="java.lang.Long"/>
		<field name="fee" class="java.math.BigDecimal"/>
		<field name="name" class="java.lang.String">
			<fieldDescription><![CDATA[租户名称]]></fieldDescription>
		</field>
		<field name="dName" class="java.lang.String"/>
		<variable name="feeSum" class="java.math.BigDecimal" calculation="Sum"/>
	</subDataset>
	<subDataset name="sumDataset" uuid="45b38015-0c3c-4a06-8942-c068911edea7">
		<parameter name="sDate" class="java.lang.String"/>
		<parameter name="eDate" class="java.lang.String"/>
		<parameter name="tlist" class="java.lang.String"/>
		<queryString>
			<![CDATA[select SUM(o.total_fee)  as sumFee
from pay_order as o,
tenant as t
where o.status='SUCCESS'
AND o.tenant_id = t.id
AND DATEDIFF(DATE_FORMAT(o.time_end,'%Y-%m-%d'), $P{sDate}) >= 0
AND DATEDIFF(DATE_FORMAT(o.time_end,'%Y-%m-%d'), $P{eDate}) <= 0
AND t.id IN ($P!{tlist})]]>
		</queryString>
		<field name="sumFee" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="startDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="endDate" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="tList" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select d.id as id
FROM
tenant as t ,
department as d
WHERE t.department_id = d.id
AND t.id IN ($P!{tList})
GROUP BY d.id
ORDER BY d.id]]>
	</queryString>
